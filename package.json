{
  "name": "redis-stream",
  "publisher": "zhaix",
  "displayName": "redis-stream",
  "description": "redis stream explorer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:redis-stream.helloWorld",
    "onCommand:redis-stream.hide"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "redis-stream.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "redis-stream.hide",
        "title": "zx hide status bar"
      },
      {
        "command": "redis-stream.connection.status",
        "title": "server info",
        "icon": "image/status.png"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "redis-stream",
          "title": "Redis Explorer",
          "icon": "image/redis-icon-0.png"
        }
      ]
    },
    "views": {
      "redis-stream": [
        {
          "id": "targetTree1",
          "name": "Stream"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "redis-stream.connection.status",
          "when": "view == targetTree1 && viewItem == connection",
          "group": "inline@0"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.51.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "ioredis": "^4.19.2"
  }
}