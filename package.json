{
  "name": "redis-stream",
  "publisher": "zhaixiang",
  "displayName": "redis-stream",
  "description": "redis stream explorer",
  "version": "1.4.3",
  "homepage": "https://github.com/runzx/redis-stream-vscode/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/runzx/redis-stream-vscode"
  },
  "bugs": {
    "url": "https://github.com/runzx/redis-stream-vscode/issues",
    "email": "843476168@qq.com"
  },
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Visualization",
    "Programming Languages"
  ],
  "keywords": [
    "redis",
    "stream",
    "mq",
    "queue",
    "manager"
  ],
  "activationEvents": [
    "onView:redisTree"
  ],
  "license": "MIT",
  "icon": "image/connection.png",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "redis-stream.connection.refresh",
        "title": "refresh redis",
        "icon": "image/change.png"
      },
      {
        "command": "redis-stream.connection.status",
        "title": "server info",
        "icon": "image/status.png"
      },
      {
        "command": "redis-stream.hide",
        "title": "server info",
        "icon": "image/status.png"
      },
      {
        "command": "redis-stream.id.value.refresh",
        "title": "value reload",
        "icon": "image/refresh.png"
      },
      {
        "command": "redis-stream.msg.value.refresh",
        "title": "value reload",
        "icon": "image/refresh.png"
      },
      {
        "command": "redis-stream.db.search",
        "title": "key search",
        "icon": "image/search.png"
      },
      {
        "command": "redis-stream.db.refresh",
        "title": "db refresh",
        "icon": "image/refresh.png"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "redis-stream",
          "title": "Redis Explorer",
          "icon": "image/redis-icon-0.png"
        }
      ]
    },
    "views": {
      "redis-stream": [
        {
          "id": "redisTree",
          "name": "Stream"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "redis-stream.connection.status",
          "when": "view == redisTree && viewItem == connection",
          "group": "inline@1"
        },
        {
          "command": "redis-stream.connection.refresh",
          "when": "view == redisTree && viewItem == connection",
          "group": "inline@0"
        },
        {
          "command": "redis-stream.id.value.refresh",
          "when": "view == redisTree && viewItem == key",
          "group": "inline@0"
        },
        {
          "command": "redis-stream.msg.value.refresh",
          "when": "view == redisTree && viewItem == group || viewItem == consumer",
          "group": "inline@0"
        },
        {
          "command": "redis-stream.db.search",
          "when": "view == redisTree && viewItem == db",
          "group": "inline@0"
        },
        {
          "command": "redis-stream.db.refresh",
          "when": "view == redisTree && viewItem == db",
          "group": "inline@1"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.51.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  },
  "dependencies": {
    "ioredis": "^4.19.2"
  }
}